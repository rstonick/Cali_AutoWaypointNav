<?xml version="1.0"?>
<launch>

  <!--- Run AMCL, Map Server & provide map (from amcl.launch) -->


  <include file="$(find rover_autonav)/launch/localization_ecst_lab.launch" />


  <arg name="no_static_map" default="false"/>

  <!-- <arg name="base_global_planner" value="global_planner/GlobalPlanner" /> -->
  <arg name="base_global_planner" default="navfn/NavfnROS"/>
  <arg name="base_local_planner" default="teb_local_planner/TebLocalPlannerROS"/>
  <!-- <arg name="base_local_planner" default="dwa_local_planner/DWAPlannerROS"/> -->
  <!-- <arg name="base_local_planner" default="base_local_planner/TrajectoryPlannerROS"/> -->

  <!-- <arg name="jason" default="robot/cmd_vel"/> -->

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

    <!-- <remap from="cmd_vel" to="$(arg jason)" /> -->

    <param name="base_global_planner" value="$(arg base_global_planner)"/>
    <param name="base_local_planner" value="$(arg base_local_planner)"/>  
    <rosparam file="$(find rover_autonav)/config/planners.yaml" command="load"/> <!--PP/my_move_base_params.yaml-->

    
   
    <!-- observation sources located in costmap_common.yaml -->
    <rosparam file="$(find rover_autonav)/config/costmap_common.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find rover_autonav)/config/costmap_common.yaml" command="load" ns="local_costmap" />

    <!-- local costmap, needs size -->
    <rosparam file="$(find rover_autonav)/config/costmap_local.yaml" command="load"  />
    <param name="local_costmap/width" value="5.0"/>
    <param name="local_costmap/height" value="5.0"/>

    <!-- static global costmap, static map provides size -->
    <rosparam file="$(find rover_autonav)/config/costmap_global_static.yaml" command="load" ns="global_costmap" unless="$(arg no_static_map)"/>
    
    <!-- global costmap with laser, for odom_navigation_demo -->
    <!-- <rosparam file="$(find rover_autonav)/config/costmap_global_laser.yaml" command="load" ns="global_costmap" if="$(arg no_static_map)"/> -->
    <param name="global_costmap/width" value="100.0" if="$(arg no_static_map)"/>
    <param name="global_costmap/height" value="100.0" if="$(arg no_static_map)"/>
  </node>

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find rover_autonav)/rviz_config/navigation_teb.rviz" />


</launch>